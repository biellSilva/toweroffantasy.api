model Banner {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  updated_at   DateTime @updatedAt @db.Timestamptz(6)
  imitation_id String
  weapon_id    String
  suit_id      String
  start_at     DateTime @db.Timestamptz(6)
  end_at       DateTime @db.Timestamptz(6)
  link         String
  limited_only Boolean  @default(false)
  is_rerun     Boolean  @default(false)
  is_collab    Boolean  @default(false)
  final_rerun  Boolean  @default(false)
}

view BannerOrderedView {
  id           Int      @id @default(autoincrement())
  created_at   DateTime @default(now()) @db.Timestamptz(6)
  updated_at   DateTime @updatedAt @db.Timestamptz(6)
  position     Int
  imitation_id String
  weapon_id    String
  suit_id      String
  start_at     DateTime @db.Timestamptz(6)
  end_at       DateTime @db.Timestamptz(6)
  link         String
  limited_only Boolean  @default(false)
  is_rerun     Boolean  @default(false)
  is_collab    Boolean  @default(false)
  final_rerun  Boolean  @default(false)
}
